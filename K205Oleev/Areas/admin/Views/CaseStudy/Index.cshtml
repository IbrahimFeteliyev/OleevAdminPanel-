@model IEnumerable<CaseStudyLanguage>




<!-- DataTales Example -->
                    <div class="card shadow mb-4">
                        <div class="card-header py-3">
                            <h6 class="m-0 font-weight-bold text-primary"><a asp-action="create">Create</a></h6>
                        </div>
                        <div class="card-body">
                            <div class="table-responsive">
                                <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                                    <thead> 
                                        <tr> 
                                            <th>Name</th> 
                                             <th>SEO</th> 
                                            <th>Photo</th> 
                                            <th>PhotoTitle</th> 
                                            <th></th> 
                                            
                                        </tr> 
                                    </thead> 
                                    <tfoot> 
                                        <tr> 
                                            <th>Name</th> 
                                             <th>SEO</th> 
                                            <th>Photo</th> 
                                            <th>PhotoTitle</th> 

                                            <th></th> 
                                             
                                        </tr> 
                                    </tfoot> 
                                    <tbody> 
                                        @foreach (var item in Model)
	                                     {
		                                     <tr> 
                                                <td>@item.Title</td> 
                                                <td>@item.SEO</td>
                                                <td>
                                                    <img width="100" src="@item.CaseStudy.PhotoURL"/>
                                                </td>  
                                                <td>@item.PhotoTitle</td> 
                                              
                                                <td>
                                                    
                                                    <a href="/admin/casestudy/edit/@item.CaseStudy.ID" class="btn btn-warning">Edit</a>
                                                </td> 
                                            </tr> 
	                                     }
                                        
                                  
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>